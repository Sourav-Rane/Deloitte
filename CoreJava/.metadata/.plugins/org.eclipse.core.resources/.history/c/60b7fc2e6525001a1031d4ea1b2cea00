package demo;

import java.util.Scanner;

public class Demo1 {

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int a , b;
		a = sc.nextInt();
		b = sc.nextInt();
		
		System.out.println("a and b before : "+a+" "+b);
		
		int t;
		t = a;
		a = b;
		b = t;
		
		System.out.println("a and b after : "+a+" "+b);
	}

}
